{
    "builds": [
        {
        "src": "fifaleagueweb/wsgi.py",
        "use": "@vercel/python"
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "fifaleagueweb/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}